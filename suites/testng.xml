<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="selenium">

    <test name="mixed">
        <parameter name="browser" value="opera" />
        <classes>
            <class name="com.academy.autotests.MixedTests" />
        </classes>
    </test>

    <test name="login">
        <parameter name="browser" value="firefox" />
        <classes>
            <class name="com.academy.autotests.LoginTests" />
        </classes>
    </test>

</suite>